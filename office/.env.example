OFFICE_DB_PORT=3306
OFFICE_DB_USER=YOUR_OFFICE_DB_USER
OFFICE_DB_PASSWORD=YOUR_OFFICE_DB_PASSWORD
OFFICE_DB_CHARSET=utf8mb4
OFFICE_DB_TIMEZONE=+09:00
OFFICE_DB_NAME_PARTNERS=YOUR_PARTNERS_DB_NAME
OFFICE_DB_NAME_BOOKING=YOUR_BOOKING_DB_NAME
OFFICE_DB_NAME_USIN=YOUR_USIN_DB_NAME
OFFICE_DB_NAME_SUBSCRIPTION=YOUR_SUBSCRIPTION_DB_NAME
OFFICE_DB_NAME_MALL=YOUR_MALL_DB_NAME
